# Frontend Environment Variables
VITE_API_URL=https://live.colourstream.example.com/api
VITE_WEBRTC_WS_HOST=live.colourstream.example.com
VITE_WEBRTC_WS_PORT=3334
VITE_WEBRTC_WS_PROTOCOL=wss
VITE_WEBRTC_APP_PATH=app
VITE_VIDEO_URL=https://video.colourstream.example.com/join
VITE_OVENPLAYER_SCRIPT_URL=https://cdn.jsdelivr.net/npm/ovenplayer/dist/ovenplayer.js

# TURN Server
VITE_TURN_SERVER_ENABLED=true
VITE_TURN_SERVER_USERNAME=colourstream
VITE_TURN_SERVER_CREDENTIAL=you_turn_server_credential_here

# OIDC Configuration
VITE_OIDC_AUTH_ENDPOINT=https://your-oidc-provider.example.com/authorize

# S3 Integration
VITE_S3_PUBLIC_ENDPOINT=https://s3.colourstream.example.com
VITE_S3_REGION=us-east-1
VITE_S3_BUCKET=uploads

# Cloud Storage Integration
VITE_ENABLE_DROPBOX=true
VITE_ENABLE_GOOGLE_DRIVE=false
VITE_GOOGLE_DRIVE_CLIENT_ID=your_google_drive_client_id_here
VITE_GOOGLE_DRIVE_API_KEY=your_google_drive_api_key_here
VITE_GOOGLE_DRIVE_APP_ID=your_google_drive_app_id_here
